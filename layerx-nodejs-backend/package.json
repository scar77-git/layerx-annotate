{
  "name": "layerx-nodejs",
  "version": "0.0.1",
  "description": "NodeJs Backend of LayerX",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check",
    "lint:fix": "npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t annotation-manager .",
    "docker:run": "docker run -p 3000:3000 -d annotation-manager",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "pm2 start ecosystem.config.js --env development",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build",
    "stop": "pm2 stop ecosystem.config.js --env development"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Kelum <kelum@racerunner.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^7.3.2",
    "@loopback/authentication-jwt": "^0.9.2",
    "@loopback/boot": "^3.4.2",
    "@loopback/core": "^2.16.2",
    "@loopback/cron": "^0.6.1",
    "@loopback/repository": "^3.7.1",
    "@loopback/rest": "^10.0.0",
    "@loopback/rest-explorer": "^3.3.2",
    "@loopback/service-proxy": "^3.2.2",
    "@sendgrid/mail": "^7.6.0",
    "archiver": "^5.3.0",
    "aws-sdk": "^2.979.0",
    "axios": "^0.21.1",
    "canvas": "^2.8.0",
    "dotenv": "^10.0.0",
    "email-validator": "^2.0.4",
    "form-data": "^4.0.0",
    "fs": "0.0.1-security",
    "fs-extra": "^10.0.0",
    "googleapis": "^84.0.0",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "loopback-connector-mongodb": "^5.5.0",
    "mongodb": "^4.1.1",
    "multer": "^1.4.3",
    "nodemailer": "^6.6.3",
    "nthline": "^1.0.2",
    "pm2": "^5.1.1",
    "randomcolor": "^0.6.2",
    "sharp": "^0.29.3",
    "short-unique-id": "^4.4.4",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^7.0.0",
    "@loopback/testlab": "^3.4.2",
    "@types/archiver": "^5.3.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mongodb": "^4.0.7",
    "@types/multer": "^1.4.7",
    "@types/node": "^10.17.60",
    "source-map-support": "^0.5.19",
    "typescript": "~4.3.4"
  }
}
